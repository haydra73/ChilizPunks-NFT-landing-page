{% extends "./templates/layout.nunjucks" %} {% block title %}Home{% endblock %}
{% block css %} {% endblock %} {% block content %}

<main id="Home">
 
  <img src="assets/img/welcome.png" alt="" class="welcome" />

  <h1 class="football-title">FOOTBALL BLOCKCHAIN</h1>
  <div class="countdown mt-medium mb-small">
    <div class="parallax home__buy" data-rellax-speed="0">
      <div class="countdown">
        <h4 class="countdown__header"></h4>
        <div id="countdown">
          <ul>
            <li class="calendar">
              <img src="assets/img/calender.png" alt="" />
              <div class="counter">
                <p>Days</p>
                <span id="days"></span>
              </div>
            </li>
            <li class="calendar">
              <img src="assets/img/calender.png" alt="" />
              <div class="counter">
                <p>Hours</p>
                <span id="hours"></span>
              </div>
            </li>
            <li class="calendar">
              <img src="assets/img/calender.png" alt="" />
              <div class="counter">
                <p>Minutes</p>
                <span id="minutes"></span>
              </div>
            </li>
          </ul>
        </div>
      </div>

      <div class="buy-btn">
        <img src="assets/img/buttonCyan.png" alt="btn" />
        <span>Buy</span>
      </div>
    </div>
  </div>
</main>

